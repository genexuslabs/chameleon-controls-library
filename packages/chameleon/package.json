  "scripts": {
    "start": "vite --config vite.config-serve.ts",
    "build": "concurrently 'bun build:dev:browser' 'bun build:dev:node' 'bun build:prod:browser' 'bun build:prod:node' 'bun build:minified' 'bun build.types'",
    "build:dev": "concurrently 'bun build:dev:browser' 'bun build:dev:node'",
    "build:dev:browser": "vite build --mode browser-development",
    "build:dev:node": "vite build --mode node-development",
    "build:prod:browser": "vite build --mode browser-production",
    "build:prod:node": "vite build --mode node-production",
    "build:minified": "vite build --mode minified",
    "build:preview": "bun run clear.dist && concurrently 'bun build:dev:browser' 'bun build:dev:node' 'bun build:prod:browser' 'bun build:prod:node'",
    "build.types": "tsgo --project tsconfig-types.json",
    "watch": "bun clear.dist && concurrently 'bun build:dev:browser --watch' 'bun build:dev:node --watch' 'bun build:prod:browser --watch' 'bun build:prod:node --watch' 'bun build.types --watch'",
    "clear.dist": "bun run ./scripts/remove-dist.ts",
    "test": "bunx playwright install && vitest",
    "test.watch": "bunx playwright install && vitest watch"
  },
  "license": "MIT",
  "peerDependencies": {
    "@genexus/kasstor-core": "^0.1.0",
    "@genexus/kasstor-signals": "^0.1.0",
    "@genexus/kasstor-webkit": "^0.2.0",
    "@open-wc/lit-helpers": "~0.7.0",
