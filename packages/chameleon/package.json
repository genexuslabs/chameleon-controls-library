    "qr-creator": "^1.0.0",
    "shiki": "^3.4.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@genexus/vite-plugin-kasstor": "~0.1.1",
    "@genexus/vite-plugin-mercury": "~1.2.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
