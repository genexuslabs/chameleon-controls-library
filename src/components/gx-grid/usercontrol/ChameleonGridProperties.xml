							<Id>PaginatorInfoShow</Id>
							<Name>PaginatorInfoShow</Name>
							<Type>Boolean</Type>
							<Default>true</Default>
						</Prop>
						<Prop>
							<Id>PaginatorInfoClass</Id>
							<Name>PaginatorInfoClass</Name>
							<Type>Text</Type>
							<Default>grid-paging-info</Default>
						</Prop>
