<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Object id="ChameleonGrid">
		<Group>
			<Name>General</Name>
			<Type>Main</Type>
			<Childs>
				<Group>
				  <Name>Classes</Name>
				  <Childs>
					<Prop>
					  <Id>ColumnsetClass</Id>
					  <Name>ColumnsetClass</Name>
					  <Type>Text</Type>
					  <Default>grid-columnset</Default>
					</Prop>
					<Prop>
					  <Id>ColumnClass</Id>
					  <Name>ColumnClass</Name>
					  <Type>Text</Type>
					  <Default>grid-column</Default>
					</Prop>	
					<Prop>
					  <Id>RowClass</Id>
					  <Name>RowClass</Name>
					  <Type>Text</Type>
					  <Default>grid-row</Default>
					</Prop>	
					<Prop>
					  <Id>RowEvenClass</Id>
					  <Name>RowEvenClass</Name>
					  <Type>Text</Type>
					  <Default></Default>
					</Prop>	
					<Prop>
					  <Id>RowOddClass</Id>
					  <Name>RowOddClass</Name>
					  <Type>Text</Type>
					  <Default></Default>
					</Prop>	
					<Prop>
					  <Id>RowSelectedClass</Id>
					  <Name>RowSelectedClass</Name>
					  <Type>Text</Type>
					  <Default>gx-row-selected</Default>
					</Prop>
					<Prop>
					  <Id>RowHighlightedClass</Id>
					  <Name>RowHighlightedClass</Name>
					  <Type>Text</Type>
					  <Default>grid-row-highlighted</Default>
					</Prop>
					<Prop>
					  <Id>CellClass</Id>
					  <Name>CellClass</Name>
					  <Type>Text</Type>
					  <Default>grid-cell</Default>
					</Prop>	
				  </Childs>
				</Group>
				<Group>
					<Name>Actionbar</Name>
					<Childs>
						<Prop>
							<Id>ActionbarShow</Id>
							<Name>ActionbarShow</Name>
							<Type>Boolean</Type>
							<Default>true</Default>
						</Prop>
						<Group>
							<Name>Refresh</Name>
							<Childs>
								<Prop>
									<Id>ButtonRefreshClass</Id>
									<Name>ButtonRefreshClass</Name>
									<Type>Text</Type>
									<Default>button-refresh</Default>
								</Prop>	
								<Prop>
									<Id>ButtonRefreshTextPosition</Id>
									<Name>ButtonRefreshTextPosition</Name>
									<Type>Combo</Type>
									<Default>title</Default>
									<Values>
										<Value id='title' desc='Tooltip'/>
										<Value id='text' desc='Text'/>
									</Values>
								</Prop>
							</Childs>
						</Group>
						<Group>
							<Name>Settings</Name>
							<Childs>
								<Prop>
									<Id>ButtonSettingsClass</Id>
									<Name>ButtonSettingsClass</Name>
									<Type>Text</Type>
									<Default>button-settings</Default>
								</Prop>	
								<Prop>
									<Id>ButtonSettingsTextPosition</Id>
									<Name>ButtonSettingsTextPosition</Name>
									<Type>Combo</Type>
									<Default>title</Default>
									<Values>
										<Value id='title' desc='Tooltip'/>
										<Value id='text' desc='Text'/>
									</Values>
								</Prop>
							</Childs>
						</Group>
					</Childs>
				</Group>
				<Group>
					<Name>Paginator</Name>
					<Childs>
						<Prop>
							<Id>PaginatorShow</Id>
							<Name>PaginatorShow</Name>
							<Type>Boolean</Type>
							<Default>true</Default>
						</Prop>
						<Prop>
							<Id>PaginatorNavigationButtonTextPosition</Id>
							<Name>PaginatorNavigationButtonTextPosition</Name>
							<Type>Combo</Type>
							<Default>title</Default>
							<Values>
								<Value id='title' desc='Tooltip'/>
								<Value id='text' desc='Text'/>
							</Values>
						</Prop>
					</Childs>
				</Group>
			</Childs>
		</Group>
	</Object>
</Content>