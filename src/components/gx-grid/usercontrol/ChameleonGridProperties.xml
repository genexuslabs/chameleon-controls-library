<?xml version="1.0"?>
<Content xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Object id="ChameleonGrid">
		<Group>
			<Name>General</Name>
			<Type>Main</Type>
			<Childs>
				<Prop>
					<Id>ControlState</Id>
					<Name>ControlState</Name>
					<Type>Custom</Type>
					<CustomType>Artech.Genexus.Common.CustomTypes.ControlAttVarReference, Artech.Genexus.Common</CustomType>
					<Metadata>
						<Value name="FlagSetValueMethod">setControlState</Value>
						<Value name="FlagGetValueMethod">getControlState</Value>
						<Value name="FlagHideVariables">false</Value>
						<Value name="FlagHideAttributes">true</Value>
					</Metadata>
				</Prop>
				<Prop>
					<Id>SortMode</Id>
					<Name>SortMode</Name>
					<Type>Combo</Type>
					<Default>client</Default>
					<Values>
						<Value id='client' desc='Client side'/>
						<Value id='server' desc='Server side'/>
					</Values>
				</Prop>
				<Group>
				  <Name>Classes</Name>
				  <Childs>
					<Prop>
					  <Id>ColumnsetClass</Id>
					  <Name>ColumnsetClass</Name>
					  <Type>Text</Type>
					  <Default>grid-columnset</Default>
					</Prop>
					<Prop>
					  <Id>ColumnClass</Id>
					  <Name>ColumnClass</Name>
					  <Type>Text</Type>
					  <Default>grid-column</Default>
					</Prop>	
					<Prop>
					  <Id>ColumnFilterClass</Id>
					  <Name>ColumnFilterClass</Name>
					  <Type>Text</Type>
					  <Default>grid-column-filter</Default>
					</Prop>	
					<Prop>
					  <Id>RowClass</Id>
					  <Name>RowClass</Name>
					  <Type>Text</Type>
					  <Default>grid-row</Default>
					</Prop>	
					<Prop>
					  <Id>RowEvenClass</Id>
					  <Name>RowEvenClass</Name>
					  <Type>Text</Type>
					  <Default></Default>
					</Prop>	
					<Prop>
					  <Id>RowOddClass</Id>
					  <Name>RowOddClass</Name>
					  <Type>Text</Type>
					  <Default></Default>
					</Prop>	
					<Prop>
					  <Id>RowSelectedClass</Id>
					  <Name>RowSelectedClass</Name>
					  <Type>Text</Type>
					  <Default>gx-row-selected</Default>
					</Prop>
					<Prop>
					  <Id>RowHighlightedClass</Id>
					  <Name>RowHighlightedClass</Name>
					  <Type>Text</Type>
					  <Default>grid-row-highlight</Default>
					</Prop>
					<Prop>
					  <Id>CellClass</Id>
					  <Name>CellClass</Name>
					  <Type>Text</Type>
					  <Default>grid-cell</Default>
					</Prop>	
