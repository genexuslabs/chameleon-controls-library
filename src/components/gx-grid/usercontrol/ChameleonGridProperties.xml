				</Group>
				<Group>
					<Name>Settings</Name>
					<Childs>
						<Prop>
							<Id>SettingsCloseTextPosition</Id>
							<Name>SettingsCloseTextPosition</Name>
							<Type>Combo</Type>
							<Default>title</Default>
							<Values>
								<Value id='title' desc='Tooltip'/>
								<Value id='text' desc='Text'/>
							</Values>
						</Prop>
					</Childs>
				</Group>
				<Group>
					<Name>Filters</Name>
					<Childs>
						<Prop>
							<Id>FilterButtonApplyText</Id>
							<Name>FilterButtonApplyText</Name>
							<Type>Text</Type>
							<Default>Apply</Default>
						</Prop>
						<Prop>
							<Id>FilterButtonResetText</Id>
							<Name>FilterButtonResetText</Name>
							<Type>Text</Type>
							<Default>Reset</Default>
						</Prop>
					</Childs>
				</Group>
				<Group>
					<Name>Actionbars</Name>
					<Childs>
						<Prop>
							<Id>ActionbarHeaderClass</Id>
							<Name>ActionbarTopClass</Name>
							<Type>Text</Type>
							<Default>grid-actionbar-header</Default>
