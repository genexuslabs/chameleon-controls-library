							<Id>PaginatorInfoTextMask</Id>
							<Name>PaginatorInfoTextMask</Name>
							<Type>Text</Type>
							<Default>{record-start} - {record-end} out of {record-count}</Default>
						</Prop>
						<Prop>
							<Id>PaginatorInfoEmptyTextMask</Id>
							<Name>PaginatorInfoEmptyTextMask</Name>
							<Type>Text</Type>
							<Default>{record-count} records</Default>
						</Prop>
