										<Value id='text' desc='Text'/>
									</Values>
								</Prop>
							</Childs>
						</Group>
						<Group>
							<Name>Action Settings</Name>
							<Childs>
								<Prop>
									<Id>ActionSettingsPosition</Id>
									<Name>ActionSettingsPosition</Name>
									<Type>Combo</Type>
									<Default>footer</Default>
									<Values>
										<Value id='none' desc='None'/>
										<Value id='header' desc='Header'/>
										<Value id='footer' desc='Footer'/>
									</Values>
								</Prop>
								<Prop>
									<Id>ActionSettingsClass</Id>
									<Name>ActionSettingsClass</Name>
									<Type>Text</Type>
									<Default>button-settings</Default>
								</Prop>	
								<Prop>
									<Id>ActionSettingsTextPosition</Id>
									<Name>ActionSettingsTextPosition</Name>
									<Type>Combo</Type>
									<Default>title</Default>
									<Values>
										<Value id='title' desc='Tooltip'/>
										<Value id='text' desc='Text'/>
									</Values>
								</Prop>
							</Childs>
						</Group>
					</Childs>
				</Group>
				<Group>
					<Name>Paginator</Name>
					<Childs>
						<Prop>
							<Id>PaginatorShow</Id>
							<Name>PaginatorShow</Name>
							<Type>Boolean</Type>
							<Default>true</Default>
						</Prop>
						<Prop>
							<Id>PaginatorNavigationButtonTextPosition</Id>
							<Name>PaginatorNavigationButtonTextPosition</Name>
							<Type>Combo</Type>
							<Default>title</Default>
							<Values>
								<Value id='title' desc='Tooltip'/>
								<Value id='text' desc='Text'/>
							</Values>
						</Prop>
						<Prop>
							<Id>PaginatorPagesShow</Id>
							<Name>PaginatorPagesShow</Name>
							<Type>Boolean</Type>
							<Default>true</Default>
						</Prop>
						<Prop>
							<Id>PaginatorPagesMaxSize</Id>
							<Name>PaginatorPagesMaxSize</Name>
							<Type>Integer</Type>
							<Default>6</Default>
						</Prop>
